package Exercice1;

import java.util.ArrayList;

public class TacheComplexe implements Tache {
	public int nbrT,cout;                                                                           
	public String nom;
	public ArrayList <SousTache> SousTaches;
	
	public TacheComplexe(int nbrT, int cout, String nom) {
		super();
		this.nbrT = nbrT;
		this.cout = cout;
		this.nom = nom;
		SousTaches = new ArrayList<>();
	} 
	
}
