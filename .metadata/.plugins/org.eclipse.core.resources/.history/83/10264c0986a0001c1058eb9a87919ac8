package Exercice2;

import java.util.ArrayList;
import java.util.Collections;

public class Stats {
private ArrayList<Etudiant> etudiantsList;

public Stats() {
	this.etudiantsList = new ArrayList<etudiantsList>();  
}

void trierParMat()
{
	Collections.sort(etudiantsList);
}
public void ajout(Etudiant e) {
	etudiantsList.add(e);
}

@Override
public String toString() {
	return "Stats [etudiantsList=" + etudiantsList + "]";
}
}
