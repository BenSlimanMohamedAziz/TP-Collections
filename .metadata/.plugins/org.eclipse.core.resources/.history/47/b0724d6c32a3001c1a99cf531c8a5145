package Exercice1;

import java.util.ArrayList;

public class TacheComplexe implements Tache {
	public int nbrT,cout;                                                                           
	public String nom;
	public ArrayList <SousTache> SousTaches;
	
	public TacheComplexe(int nbrT, int cout, String nom) {
		super();
		this.nbrT = nbrT;
		this.cout = cout;
		this.nom = nom;
		SousTaches = new ArrayList<>();
	} 
	
	@Override
public String toString() {
		return "TacheComplexe [nbrT=" + nbrT + ", cout=" + cout + ", nom=" + nom + "]";
	}

public int getNbrT() {
		return nbrT;
	}

public void setNbrT(int nbrT) {
		this.nbrT = nbrT;
	}
	
	@Override
public int getCout() {
		return cout;
	}

public void setCout(int cout) {
		this.cout = cout;
	}
	
	@Override
public String getNom() {
		return nom;
	}

public void setNom(String nom) {
		this.nom = nom;
	}    
}

