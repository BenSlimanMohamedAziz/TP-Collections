package Exercice1;

import java.util.ArrayList;


public class TacheElementaire {
	public double matricule;                                                                           
	public String nom;                                                                                 
	public ArrayList <SousTache> SousTaches; 
	
	public TacheElementaire(double matricule, String nom) {                                                    
		super();                                                                                       
		this.matricule = matricule;                                                                    
		this.nom = nom;                                                                                
		SousTache = new ArrayList<>();                                                                     
	}    
}
