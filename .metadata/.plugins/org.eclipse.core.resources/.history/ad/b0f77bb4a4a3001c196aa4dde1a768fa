package Ex2Vector;

import java.util.Comparator;

import Exercice2.Etudiant;

public class MoyComparator implements Comparator<Etudiant> {
	
	@Override
	public int compare(Etudiant o1, Etudiant o2) {
		if(o1.getAvg()==o2.getAvg())
			return 0;
		else if(o1.getAvg()>o2.getAvg())
			return 1;
		else
			return -1;
		
	}
}
