package Exercice1;

import java.util.ArrayList;


public class TacheElementaire {
	public int nbrT,cout;                                                                           
	public String nom;
	public ArrayList <SousTache> SousTaches; 
	
	public TacheElementaire(int cout, int cout, String nom) {                                                    
		super();                                                                                       
		this.nbrT = nbrT;     
		this.cout = cout;     
		this.nom = nom;                                                                                
		SousTache = new ArrayList<>();                                                                     
	}    
}
